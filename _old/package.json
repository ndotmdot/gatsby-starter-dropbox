{
  "name": "gatsby-starter-dropbox",
  "private": true,
  "description": "Build Pages from Dropbox folder with automated deployment to netlify",
  "author": "ndotmdot",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "NODE_ENV='development' netlify-lambda serve src/functions",
    "build": "gatsby build && cd src/functions yarn install && cd .. netlify-lambda build src/functions",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions",
    "clean": "gatsby clean",
    "tunnel": "cd $HOME; ./ngrok http -subdomain=callbuildhook 9000"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^2.21.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-source-dropbox": "^0.1.1",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.3",
    "gatsby-transformer-sharp": "^2.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "http-proxy-middleware": "^1.0.3",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
